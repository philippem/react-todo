{
  "plugins": [
    "transform-runtime",
    "transform-decorators-legacy",
    "transform-class-properties",
    "transform-object-rest-spread"
  ],
  "presets": [
    ["env", {"targets": {"node": "current"}}],
    "react"
  ],
  "sourceMaps": true,
  "retainLines": true,
  "env": {
    "instrument": {
      "plugins": [
        "istanbul"
      ]
    }
  }
}
