{
  "name": "react-todo",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "coverage": "npm run clean:coverage && nyc npm test",
    "clean:dist": "rimraf dist",
    "clean:tmp": "rimraf .tmp",
    "clean:coverage": "rimraf coverage && rimraf .nyc_output",
    "test": "npm run clean:tmp && cross-env NODE_ENV=test DATA_PATH=./.tmp/data mocha-webpack",
    "lint": "eslint . --ext=js,jsx",
    "build": "npm run clean:dist && webpack",
    "build-watch": "npm run clean:dist && webpack -w",
    "start": "cross-env DATA_PATH=./data/ node dist/server",
    "start-watch": "cross-env DATA_PATH=./data/ nodemon --debug=5858 dist/server"
  },
  "author": "Tyler Waters",
  "license": "MIT",
  "devDependencies": {
    "async": "^2.1.4",
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.1.0",
    "babel-loader": "^6.2.7",
    "babel-plugin-istanbul": "^3.0.0",
    "babel-plugin-transform-class-properties": "^6.18.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "cross-env": "^3.1.3",
    "css-loader": "^0.25.0",
    "eslint": "^3.10.0",
    "eslint-plugin-react": "^6.6.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.24.1",
    "inject-loader": "^3.0.0-beta2",
    "json-loader": "^0.5.4",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "mocha": "^3.2.0",
    "mocha-webpack": "^0.7.0",
    "nodemon": "^1.11.0",
    "nyc": "^10.0.0",
    "rimraf": "^2.5.4",
    "sinon": "^1.17.6",
    "sinon-as-promised": "^4.0.2",
    "source-map-support": "^0.4.6",
    "style-loader": "^0.13.1",
    "supertest": "^2.0.1",
    "webpack": "^1.13.3",
    "webpack-node-externals": "^1.5.4"
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "ejs": "^2.5.2",
    "express": "^4.14.0",
    "express-session": "^1.14.2",
    "fs-extra": "^1.0.0",
    "hashifier": "^1.0.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "uuid": "^3.0.1"
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "**/test/**",
      "**/webpack/**"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  }
}
