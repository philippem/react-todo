{
  "name": "react-todo",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "coverage": "npm run clean:coverage && nyc npm test",
    "clean:dist": "rimraf dist",
    "clean:tmp": "rimraf .tmp",
    "clean:coverage": "rimraf coverage && rimraf .nyc_output",
    "test": "npm run clean:tmp && cross-env NODE_ENV=test mocha-webpack --exit",
    "lint": "eslint . --ext=js,jsx",
    "build": "npm run clean:dist && cross-env NODE_ENV=production webpack --config webpack.config-prod.js",
    "build-watch": "npm run clean:dist && cross-env NODE_ENV=development webpack -w --progress",
    "start": "node dist/server/server.js",
    "start-watch": "nodemon --inspect=5858 dist/server/server.js"
  },
  "author": "Tyler Waters",
  "license": "MIT",
  "devDependencies": {
    "async": "^2.1.4",
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.1.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-istanbul": "^3.0.0",
    "babel-plugin-transform-class-properties": "^6.18.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "bootstrap": "^3.3.7",
    "cross-env": "^3.1.3",
    "css-loader": "^0.28.4",
    "env-file-parser": "^1.0.0",
    "eslint": "^3.10.0",
    "eslint-plugin-react": "^6.6.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.24.1",
    "inject-loader": "^3.0.0",
    "json-loader": "^0.5.4",
    "less": "^2.7.1",
    "less-loader": "^4.0.5",
    "mocha": "^3.2.0",
    "mocha-webpack": "^0.7.0",
    "nodemon": "^1.11.0",
    "nyc": "^10.0.0",
    "rimraf": "^2.5.4",
    "sinon": "^1.17.6",
    "sinon-as-promised": "^4.0.2",
    "source-map-support": "^0.4.6",
    "stats-webpack-plugin": "^0.6.1",
    "strip-sourcemap-loader": "0.0.1",
    "style-loader": "^0.13.1",
    "supertest": "^2.0.1",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "webpack": "^3.3.0",
    "webpack-node-externals": "^1.5.4"
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "classnames": "^2.2.5",
    "connect-flash": "^0.1.1",
    "ejs": "^2.5.2",
    "express": "^4.14.0",
    "express-session": "^1.14.2",
    "font-awesome": "^4.7.0",
    "fs-extra": "^1.0.0",
    "hashifier": "^1.0.0",
    "history": "^4.6.1",
    "isomorphic-fetch": "^2.2.1",
    "prop-types": "^15.5.8",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-helmet": "^5.0.3",
    "react-intl": "^2.3.0",
    "react-intl-redux": "^0.6.0",
    "react-redux": "^5.0.5",
    "react-router": "^4.0.0",
    "react-router-dom": "^4.0.0",
    "react-router-redux": "^5.0.0-alpha.4",
    "react-router-server": "^4.2.0",
    "redux": "^3.6.0",
    "redux-logger": "^2.8.2",
    "redux-thunk": "^2.1.0",
    "reselect": "^2.5.4",
    "session-file-store": "^1.0.0",
    "uuid": "^3.0.1"
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "**/test/**",
      "**/webpack/**"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  }
}
