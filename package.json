{
  "name": "react-todo",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean:dist": "rimraf dist",
    "clean:tmp": "rimraf .tmp",
    "clean:coverage": "rimraf coverage && rimraf .nyc_output",
    "clean:db": "rimraf data/*.sqlite",
    "precoverage": "npm run clean:coverage",
    "coverage": "nyc npm test",
    "db": "sequelize db:migrate && sequelize db:seed:all",
    "pretest": "npm run clean:tmp && rimraf data/database-test.sqlite && cross-env NODE_ENV=test npm run db",
    "test": "cross-env NODE_ENV=test NODE_PATH=./src mocha --opts mocha.opts",
    "lint": "eslint . --ext=js,jsx",
    "build": "npm run clean:dist && cross-env NODE_ENV=production webpack --progress --config webpack.config-prod.js",
    "build-watch": "npm run clean:dist && cross-env NODE_ENV=development webpack -w --progress",
    "prestart": "cross-env NODE_ENV=production npm run db",
    "start": "npm run build && npm run start:prod",
    "prestart-watch": "cross-env NODE_ENV=development npm run db",
    "start-watch": "concurrently -r --kill-others \"npm run build-watch\" \"npm run start:dev\"",
    "start:prod": "cross-env NODE_ENV=production node dist/server/server.js",
    "start:dev": "wait-on dist/server/server.js  dist/server/stats.json && cross-env NODE_ENV=development nodemon --inspect dist/server/server.js"
  },
  "author": "Tyler Waters",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.1.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-istanbul": "^3.0.0",
    "babel-plugin-transform-class-properties": "^6.18.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.16.0",
    "bootstrap": "^3.3.7",
    "concurrently": "^3.5.0",
    "cross-env": "^3.1.3",
    "css-loader": "^0.28.4",
    "env-file-parser": "^1.0.0",
    "eslint": "^3.10.0",
    "eslint-plugin-react": "^6.6.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.24.1",
    "json-loader": "^0.5.4",
    "less": "^2.7.1",
    "less-loader": "^4.0.5",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "nyc": "^10.0.0",
    "proxyquire": "^1.8.0",
    "rimraf": "^2.5.4",
    "sinon": "^4.0.2",
    "stats-webpack-plugin": "^0.6.1",
    "style-loader": "^0.13.1",
    "supertest": "^2.0.1",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "wait-on": "^2.0.2",
    "webpack": "^3.3.0",
    "webpack-node-externals": "^1.5.4"
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "chalk": "^2.3.0",
    "classnames": "^2.2.5",
    "connect-flash": "^0.1.1",
    "connect-redis": "^3.3.2",
    "ejs": "^2.5.2",
    "express": "^4.14.0",
    "express-session": "^1.14.2",
    "font-awesome": "^4.7.0",
    "fs-extra": "^1.0.0",
    "hashifier": "^1.0.0",
    "history": "^4.6.1",
    "ioredis": "^3.2.1",
    "isomorphic-fetch": "^2.2.1",
    "pino": "^4.8.1",
    "prop-types": "^15.5.8",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-helmet": "^5.0.3",
    "react-intl": "^2.3.0",
    "react-intl-redux": "^0.6.0",
    "react-redux": "^5.0.5",
    "react-router": "^4.0.0",
    "react-router-dom": "^4.0.0",
    "react-router-redux": "^5.0.0-alpha.4",
    "react-router-server": "^4.2.0",
    "redux": "^3.6.0",
    "redux-logger": "^2.8.2",
    "redux-thunk": "^2.1.0",
    "reselect": "^2.5.4",
    "sequelize": "^4.19.0",
    "sequelize-cli": "^3.0.0",
    "source-map-support": "^0.5.0",
    "sqlite3": "^3.1.13",
    "uuid": "^3.0.1"
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "**/test/**",
      "**/webpack/**"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  }
}
